on init
    start vendor.keymaster-4-0

on property:vendor.keymaster.quickboot=true
   stop vendor.keymaster-4-0
   start vendor.keymaster-4-0
   start wait_for_keymaster

service vendor.keymaster-4-0 /vendor/bin/hw/android.hardware.keymaster@4.0-service-qti
    class early_hal
    user system
    group system drmrpc
